<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="registrationDepartment">

    <insert id="insert" keyColumn="REGD_ID" parameterType="com.estylesoft.integration.Model.Ptks.RegistrationDepartmentModel">
        insert into STH_TERRITORY_DPT_REG(REGD_ID, REGD_NAME, REGD_CODE)
        values (
        #{id},
        #{name},
        #{code})
    </insert>

    <delete id="delete" parameterType="java.lang.Long">
        delete from STH_TERRITORY_DPT_REG
        where REGD_ID = #{id}
    </delete>

</mapper>
