<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="opfr">

    <select id="getByCode" parameterType="String" resultType="com.estylesoft.integration.Model.Ptks.OpfrModel">
          SELECT MRDV_ID as id, MRDV_CODE as code
          FROM STH_MAIN_REG_DIV_PFR
          WHERE MRDV_CODE = #{code}
    </select>

<!--    <delete id="delete" parameterType="Long">
        delete from STH_MAIN_REG_DIV_PFR
        where MRDV_ID = #{id}
    </delete>-->

</mapper>
