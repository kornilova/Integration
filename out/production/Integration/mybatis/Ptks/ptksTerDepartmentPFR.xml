    <delete id="delete" parameterType="com.estylesoft.integration.Model.Ptks.TerDepartmentPFRModel">
        delete from STH_TERRITORY_DPT_PFR
        WHERE TDP_CODE=#{code} AND
        RDV_ID = #{baseDepartmentId} AND
        TER_ORGAN_ID =  #{terOrganId}
    </delete>

    <select id="getByBaseDepCode" parameterType="Map" resultType="com.estylesoft.integration.Model.Ptks.TerDepartmentPFRModel">
          SELECT TDP_ID as id, TDP_CODE as code
          FROM STH_TERRITORY_DPT_PFR
          WHERE TDP_CODE = #{code} AND RDV_ID = #{baseDepartmentId}
    </select>

    <select id="getByCode" parameterType="String" resultType="com.estylesoft.integration.Model.Ptks.TerDepartmentPFRModel">
